{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "configMigration": true,
  "extends": [
    "config:recommended"
  ],
  "mode": "full",
  "addLabels": ["renovate"],
  "timezone": "America/Chicago",
  "html": {
    "fileMatch": [
      "\\.html?$",
      "\\.jte?$"
    ]
  },
  "packageRules": [
    {
      "groupName": "jte",
      "matchPackageNames": [
        "gg.jte:jte-spring-boot-starter-3",
        "gg.jte:jte",
        "gg.jte.gradle"
      ]
    }
  ]
}
